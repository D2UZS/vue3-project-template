{
  "name": "vue-project",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "node check-node-version && vite --host",
    "build": "node check-node-version && run-p type-check build-only",
    "preview": "node check-node-version && vite preview",
    "build-only": "node check-node-version && vite build",
    "type-check": "node check-node-version && vue-tsc --noEmit",
    "lint": "node check-node-version && eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "maska": "^2.1.7",
    "pinia": "^2.0.28",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/node": "^18.11.12",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "eslint-plugin-vue-scoped-css": "^2.4.0",
    "meow": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "optionator": "^0.9.1",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.7.1",
    "sass": "^1.58.0",
    "stylelint": "^15.1.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-order": "^6.0.1",
    "typescript": "~4.7.4",
    "vite": "^4.0.0",
    "vite-plugin-checker": "^0.5.5",
    "vue-tsc": "^1.0.12"
  }
}
